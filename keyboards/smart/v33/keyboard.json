            { "flags": 4, "matrix": [0, 6], "x": 188, "y": 6 },//scroll
            { "flags": 4, "matrix": [0, 7], "x": 224, "y": 6 },//pause
      
            { "flags": 4, "matrix": [1, 6], "x": 224, "y": 20 },//pageup
            { "flags": 4, "matrix": [1, 5], "x": 188, "y": 20 },//home
